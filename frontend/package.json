{
  "name": "api-book",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "@vueuse/head": "^0.7.5",
    "axios": "^0.25.0",
    "pinia": "^2.0.11",
    "vue": "^3.2.25",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@types/node": "^17.0.16",
    "@vitejs/plugin-vue": "^2.0.0",
    "csslab": "^4.0.3",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.3.5",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "prettier-eslint --write",
